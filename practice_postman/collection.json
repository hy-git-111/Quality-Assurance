{
	"info": {
		"_postman_id": "0a14d568-d660-441a-9077-678c58d95f53",
		"name": "사용자 등록 API 테스트",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json",
		"_exporter_id": "44031903"
	},
	"item": [
		{
			"name": "회원가입",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
				},
				{
					"listen": "test",
					"script": {
						"exec": [
							"const result = pm.response.json();\r",
							"\r",
							"const expectedCode = pm.iterationData.get(\"code\");\r",
							"const expectedStatus = parseInt(pm.iterationData.get(\"status\"));\r",
							"\r",
							"pm.test(\"응답코드 확인\", () => {\r",
							"    pm.expect(result.code).to.eql(expectedCode);\r",
							"});\r",
							"\r",
							"pm.test(\"상태코드 확인\", () => {\r",
							"    pm.expect(pm.response.code).to.eql(expectedStatus);\r",
							"});"
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": "{\r\n    \"email\": \"{{EMAIL}}\",\r\n    \"password\": \"{{PASSWORD}}\",\r\n    \"name\": \"{{NAME}}\",\r\n}",
					"options": {
						"raw": {
							"language": "json"
						}
					}
				},
				"url": {
					"raw": "{{BASE_URL}}/signup",
					"host": [
						"{{BASE_URL}}"
					],
					"path": [
						"signup"
					]
				}
			},
			"response": []
		}
	]
}